# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'EggMap' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for EggMap
pod 'GoogleMaps'
  pod 'GooglePlaces'
pod 'RealmSwift'
pod 'SwiftyJSON'
pod 'Alamofire', '~> 4.7'

  target 'EggMapTests' do
    inherit! :search_paths
    # Pods for testing
pod 'GoogleMaps'
  pod 'GooglePlaces'
pod 'RealmSwift'
pod 'SwiftyJSON'
pod 'Alamofire', '~> 4.7'
  end

  target 'EggMapUITests' do
    inherit! :search_paths
    # Pods for testing
pod 'GoogleMaps'
  pod 'GooglePlaces'
pod 'RealmSwift'
pod 'SwiftyJSON'
pod 'Alamofire', '~> 4.7'
  end

end
